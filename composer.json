{
    "name": "jordanmiguel/laravel-popular",
    "type": "library",
    "description": "A Laravel package to track popularity of entries(by Models) in a website at a certain time.",
    "keywords": [
        "JordanMiguel",
        "laravel-popular",
        "Laravel",
        "Popular",
        "Laravel Popular"
    ],
    "homepage": "https://github.com/jordanmiguel/laravel-popular",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan Miguel",
            "homepage": "https://github.com/JordanMiguel",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0 || ^7.0",
        "php" : "~5.6|~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "JordanMiguel\\LaravelPopular\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JordanMiguel\\LaravelPopular\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "laravel": {
          "providers": [
            "JordanMiguel\\LaravelPopular\\LaravelPopularServiceProvider"
          ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
